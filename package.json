{
  "private": true,
  "name": "jsxtreme-markdown",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "format": "prettier --single-quote --write '{,lib/**/,test/**/}*.js'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "eslint-plugin-node": "^5.1.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "js-beautify": "^1.6.14",
    "lint-staged": "^4.0.2",
    "mkdirp": "^0.5.1",
    "pify": "^3.0.0",
    "prettier": "^1.5.3"
  },
  "engines": {
    "node": ">=4"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "clearMocks": true
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
